                                <ScrollViewer  Margin="0" x:Name="PART_ContentHost"/>
                            </Viewbox>
                        </Border>
                    </ControlTemplate>
                </TextBox.Template>
            </TextBox>
            <TextBlock x:Name="TextTask3M1Y1" HorizontalAlignment="Center" Margin="0,396,0,0" TextWrapping="Wrap" Text="Сколько существует {RandomTemp31}-значных телефонных номеров, в первых {RandomTemp32}-х цифрах которых не встречаются цифры 0 и 3?" VerticalAlignment="Top" Height="73" Width="522" FontFamily="pack://application:,,,/fonts/#Jost Regular" FontSize="16"/>
